    "def set_continent(dataframe):\n",
    "    ContinentDict  = {'China':'Asia', \n",
    "                      'United States':'North America', \n",
    "                      'Japan':'Asia', \n",
    "                      'United Kingdom':'Europe', \n",
    "                      'Russian Federation':'Europe', \n",
    "                      'Canada':'North America', \n",
    "                      'Germany':'Europe', \n",
    "                      'India':'Asia',\n",
    "                      'France':'Europe', \n",
    "                      'South Korea':'Asia', \n",
    "                      'Italy':'Europe', \n",
    "                      'Spain':'Europe', \n",
    "                      'Iran':'Asia',\n",
    "                      'Australia':'Australia', \n",
    "                      'Brazil':'South America'}\n",
    "    dataframe['Continent'] = 'Earth'\n",
    "    for country, continent in ContinentDict.items():\n",
    "        dataframe['Continent'].loc[country] = continent\n",
    "    return dataframe\n",
    "    \n",
    "\n",
    "def answer_eleven():\n",
    "    \n",
    "    Top15 = answer_one()\n",
    "    Top15['PopEst'] = (Top15['Energy Supply'] / Top15['Energy Supply per Capita']).astype(float)\n",
    "    Top15 = set_continent(Top15)\n",
    "    new_df = (Top15.groupby(['Continent']).agg({'PopEst': ['count','sum','mean','std']}))\n",
    "    new_df.rename(columns={\"count\": \"size\"}, inplace=True)\n",
    "    new_df.columns = new_df.columns.droplevel(0)\n",
    "    return new_df    \n",
    "\n",
    "\n",
    "\n",
    "# Function answer_eleven was answered incorrectly, 0.0666 points were not awarded. \n",
    "#In particular, yoursolution shape (5, 4) was not the same as the autograder solution shape (5, 3). \n"
