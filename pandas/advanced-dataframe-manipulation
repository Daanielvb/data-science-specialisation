    "def add_commas(input):\n",
    "    \n",
    "    return \"{:,}\".format(input)\n",
    "#     input = str(input)\n",
    "#     parts = input.split(\".\")\n",
    "#     n = 3\n",
    "#     begin = [parts[0][i:i+n] for i in range(0, len(parts[0]), n)]\n",
    "#     begin = ','.join(begin)\n",
    "#     return begin + \".\" + parts[1]\n",
    "\n",
    "\n",
    "def answer_thirteen():\n",
    "    Top15 = answer_one()\n",
    "    Top15['PopEst'] = (Top15['Energy Supply'] / Top15['Energy Supply per Capita'])\n",
    "    Top15['PopEst'] = Top15['PopEst']\n",
    "    Top15['PopEst'] = Top15['PopEst'].apply(add_commas)\n",
    "    return Top15['PopEst']\n",
